{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.AspNetCore", "Serilog.Sinks.Seq" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://nerdstore-seq:5341"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithEnvironmentName" ]
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=sql-server;Database=NerdStoreEnterpriseDB;MultipleActiveResultSets=true;User Id=sa;Password=SqlServer@2022"
  },
  "MessageQueueConnection": {
    "MessageBus": "host=rabbit-nerdstore:5672;publisherConfirms=true;timeout=30;username=nerdstore;password=nerdstore"
  },
  "AppTokenSettings": {
    "RefreshTokenExpiration": 8
  }
}
